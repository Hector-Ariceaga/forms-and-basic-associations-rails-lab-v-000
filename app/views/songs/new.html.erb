<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist_name, 'Artist' %>
  <%= f.text_field :artist_name %>
  <%= f.label :genre_id, 'Genre' %>
  <%= f.collection_select :genre_id, Genre.all, :name, :name %>
  <input name="note[contents][]">
  <input name="note[contents][]">
  <br />

  <%= f.submit %>
<% end %>
